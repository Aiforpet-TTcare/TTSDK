<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="54dp"
            android:background="@color/color_ffffff">


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="18dp"
                android:orientation="horizontal"
                android:layout_centerVertical="true"
                android:layout_marginRight="45dp">


                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/pet_photo_cardview"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:shape="ring"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="0dp"
                    android:layout_gravity="center_vertical"
                    app:cardPreventCornerOverlap="true">

                    <ImageView
                        android:id="@+id/pet_photo"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_gravity="center" />

                </com.google.android.material.card.MaterialCardView>


                <TextView
                    style="@style/Title_Large"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    android:textColor="@color/color_191919"
                    android:id="@+id/pet_name"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="7dp"/>




            </LinearLayout>


            <TextView
                android:id="@+id/close"
                style="@style/Font_Medium_14"
                android:textSize="13dp"
                android:layout_width="56dp"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:gravity="center_horizontal"
                android:paddingTop="15dp"
                android:text="@string/eye_check_4"
                android:textColor="@color/color_000000" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/main_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <androidx.camera.view.PreviewView
                android:id="@+id/view_finder"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <RelativeLayout
                android:id="@+id/noiseview_parent"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:visibility="gone">

                <ImageView
                    android:id="@+id/noiseview"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/noisebg" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#af000000" />
            </RelativeLayout>


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/camera_gradient" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/camera_gradient2" />


            <RelativeLayout
                android:id="@+id/pick_completeview"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#4c000000"
                android:visibility="gone">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/camera_complete1"
                        style="@style/Title_Large"
                        android:textSize="23dp"
                        android:layout_gravity="center_horizontal"
                        android:gravity="center"
                        android:text="@string/camera_complete1"
                        android:textColor="#ffffff" />

                    <TextView
                        android:id="@+id/camera_complete2"
                        style="@style/Font_Regular_13"
                        android:textSize="12dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="25.2dp"
                        android:gravity="center"
                        android:text="@string/camera_complete2"
                        android:textColor="#ffffff" />


                    <LinearLayout
                        android:id="@+id/refresh_btn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="54dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="12.24dp"
                            android:layout_height="10.08dp"
                            android:layout_gravity="center_vertical"
                            android:background="@drawable/refresh" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginLeft="3.6dp"
                            android:orientation="vertical">

                            <TextView
                                style="@style/Font_Regular_13_type2"
                                android:textSize="12dp"
                                android:layout_gravity="center_vertical"
                                android:text="@string/camera_refresh"
                                android:textColor="#ffffff" />


                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="#ffffff" />


                        </LinearLayout>


                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="64.8dp" />

                </LinearLayout>


            </RelativeLayout>


            <RelativeLayout
                android:layout_width="203.4dp"
                android:layout_height="203.4dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="100dp">


                <ImageView
                    android:id="@+id/focusview"
                    android:layout_width="203.4dp"
                    android:layout_height="203.4dp"
                    android:layout_gravity="center_horizontal"
                    android:background="@drawable/frame_basic" />


                <LinearLayout
                    android:id="@+id/camera_allow_view"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/allow_camera_ment"
                        style="@style/Font_Medium"
                        android:layout_gravity="center_horizontal"
                        android:gravity="center"
                        android:text="@string/allow_camera_ment"
                        android:textColor="#ffffff"
                        android:textSize="14dp" />

                    <LinearLayout
                        android:id="@+id/camera_enable_btn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="15.7dp"
                        android:orientation="vertical">

                        <TextView
                            style="@style/Font_Medium"
                            android:layout_gravity="center_horizontal"
                            android:layout_weight="0"
                            android:gravity="center"
                            android:text="@string/allow_camera_enable_ment"
                            android:textColor="#ffffff"
                            android:textSize="14dp" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="#ffffff" />


                    </LinearLayout>


                </LinearLayout>

            </RelativeLayout>


            <TextView
                android:id="@+id/focus_ment"
                style="@style/Font_Medium_14"
                android:layout_width="match_parent"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="27dp"
                android:layout_marginTop="30dp"
                android:layout_marginRight="27dp"
                android:background="@drawable/round_16_blur"
                android:gravity="center"
                android:paddingTop="12dp"
                android:paddingBottom="12dp"
                android:textColor="#ffffff"
                android:textSize="13dp" />



            <LinearLayout
                android:id="@+id/eyemode_image_menu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:visibility="visible"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="45dp">

                <LinearLayout
                    android:layout_width="41.4dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:id="@+id/skin_menu1_parent">

                    <RelativeLayout
                        android:layout_width="41.4dp"
                        android:layout_height="24.3dp" >

                        <ImageView
                            android:id="@+id/skin_menu1_now"
                            android:layout_width="9dp"
                            android:layout_height="8.1dp"
                            android:layout_centerHorizontal="true"
                            android:layout_alignParentBottom="true"
                            android:background="@drawable/cam_now"
                            android:visibility="invisible"
                            android:layout_marginBottom="6.3dp"/>

                    </RelativeLayout>


                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="41.4dp"
                        android:layout_height="41.4dp"
                        android:shape="ring"
                        app:cardBackgroundColor="@android:color/transparent"
                        app:cardCornerRadius="14dp"
                        app:cardElevation="0dp"
                        app:cardPreventCornerOverlap="true">

                        <ImageView
                            android:id="@+id/skin_menu1"
                            android:layout_width="41.4dp"
                            android:layout_height="41.4dp" />

                    </com.google.android.material.card.MaterialCardView>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="41.4dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginLeft="7.2dp"
                    android:id="@+id/skin_menu2_parent">

                    <RelativeLayout
                        android:layout_width="41.4dp"
                        android:layout_height="24.3dp" >
                        <ImageView
                            android:id="@+id/skin_menu2_now"
                            android:layout_width="9dp"
                            android:layout_height="8.1dp"
                            android:layout_centerHorizontal="true"
                            android:layout_alignParentBottom="true"
                            android:background="@drawable/cam_now"
                            android:visibility="invisible"
                            android:layout_marginBottom="6.3dp"/>
                    </RelativeLayout>

                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="41.4dp"
                        android:layout_height="41.4dp"
                        android:shape="ring"
                        app:cardBackgroundColor="@android:color/transparent"
                        app:cardCornerRadius="14dp"
                        app:cardElevation="0dp"
                        app:cardPreventCornerOverlap="true">

                        <ImageView
                            android:id="@+id/skin_menu2"
                            android:layout_width="41.4dp"
                            android:layout_height="41.4dp" />

                    </com.google.android.material.card.MaterialCardView>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="41.4dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginLeft="7.2dp"
                    android:id="@+id/skin_menu3_parent">

                    <RelativeLayout
                        android:layout_width="41.4dp"
                        android:layout_height="24.3dp" >

                        <ImageView
                            android:id="@+id/skin_menu3_now"
                            android:layout_width="9dp"
                            android:layout_height="8.1dp"
                            android:layout_centerHorizontal="true"
                            android:layout_alignParentBottom="true"
                            android:background="@drawable/cam_now"
                            android:visibility="invisible"
                            android:layout_marginBottom="6.3dp"/>

                    </RelativeLayout>

                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="41.4dp"
                        android:layout_height="41.4dp"
                        android:shape="ring"
                        app:cardBackgroundColor="@android:color/transparent"
                        app:cardCornerRadius="14dp"
                        app:cardElevation="0dp"
                        app:cardPreventCornerOverlap="true">

                        <ImageView
                            android:id="@+id/skin_menu3"
                            android:layout_width="41.4dp"
                            android:layout_height="41.4dp" />
                    </com.google.android.material.card.MaterialCardView>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="41.4dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginLeft="7.2dp"
                    android:id="@+id/skin_menu4_parent">

                    <RelativeLayout
                        android:layout_width="41.4dp"
                        android:layout_height="24.3dp" >

                        <ImageView
                            android:id="@+id/skin_menu4_now"
                            android:layout_width="9dp"
                            android:layout_height="8.1dp"
                            android:layout_centerHorizontal="true"
                            android:layout_alignParentBottom="true"
                            android:background="@drawable/cam_now"
                            android:visibility="invisible"
                            android:layout_marginBottom="6.3dp"/>
                    </RelativeLayout>

                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="41.4dp"
                        android:layout_height="41.4dp"
                        android:shape="ring"
                        app:cardBackgroundColor="@android:color/transparent"
                        app:cardCornerRadius="14dp"
                        app:cardElevation="0dp"
                        app:cardPreventCornerOverlap="true">

                        <ImageView
                            android:id="@+id/skin_menu4"
                            android:layout_width="41.4dp"
                            android:layout_height="41.4dp" />

                    </com.google.android.material.card.MaterialCardView>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="57.6dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="57.6dp"
                        android:layout_height="24.3dp" >

                        <ImageView
                            android:id="@+id/skin_menu5_now"
                            android:layout_width="57.6dp"
                            android:layout_height="24.3dp"
                            android:layout_centerHorizontal="true"
                            android:layout_alignParentBottom="true"
                            android:background="@drawable/bubble_edit"
                            android:visibility="visible"/>
                    </RelativeLayout>

                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="41.4dp"
                        android:layout_height="41.4dp"
                        android:shape="ring"
                        android:layout_gravity="center_horizontal"
                        app:cardBackgroundColor="@android:color/transparent"
                        app:cardCornerRadius="14dp"
                        app:cardElevation="0dp"
                        app:cardPreventCornerOverlap="true">

                        <ImageView
                            android:id="@+id/skin_menu_refresh"
                            android:layout_gravity="center_horizontal"
                            android:layout_width="41.4dp"
                            android:layout_height="41.4dp"
                            android:background="@drawable/common_check_again"/>
                    </com.google.android.material.card.MaterialCardView>

                </LinearLayout>


            </LinearLayout>


        </RelativeLayout>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="171dp"
            android:background="@color/color_ffffff">


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="79.2dp"
                android:orientation="horizontal"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="33.3dp">

                <RelativeLayout
                    android:layout_width="36dp"
                    android:layout_height="36dp"
                    android:id="@+id/flash_btn"
                    android:layout_gravity="center_vertical">

                    <ImageView
                        android:layout_width="15.3dp"
                        android:layout_height="17.1dp"
                        android:background="@drawable/common_2_icon_flash_off"
                        android:id="@+id/flash_image"
                        android:layout_centerInParent="true"/>

                </RelativeLayout>


                <RelativeLayout
                    android:layout_width="113.4dp"
                    android:layout_height="79.2dp"
                    android:layout_marginLeft="16.2dp"
                    android:layout_marginRight="16.2dp"
                    android:layout_gravity="center_horizontal">

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/pickcardview"
                        android:layout_width="55.8dp"
                        android:layout_height="55.8dp"
                        android:layout_centerInParent="true"
                        android:animateLayoutChanges="true"
                        android:visibility="visible"
                        app:cardBackgroundColor="@android:color/transparent"
                        app:cardCornerRadius="37.65dp"
                        app:cardElevation="5.4dp"
                        app:cardPreventCornerOverlap="true"
                        android:layout_centerHorizontal="true">

                        <ImageView
                            android:id="@+id/capture_button"
                            android:layout_width="55.8dp"
                            android:layout_height="55.8dp"
                            android:background="@drawable/capture_default" />


                    </com.google.android.material.card.MaterialCardView>


                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/send_btn"
                        android:layout_width="101.7dp"
                        android:layout_height="40.5dp"
                        android:layout_centerInParent="true"
                        android:animateLayoutChanges="true"
                        android:visibility="gone"
                        app:cardBackgroundColor="@android:color/transparent"
                        app:cardCornerRadius="28dp"
                        app:cardElevation="5.4dp"
                        app:cardPreventCornerOverlap="true">

                        <ImageView
                            android:layout_width="101.7dp"
                            android:layout_height="42.3dp"
                            android:background="@drawable/common_check_cam_result"
                            android:id="@+id/camera_lens_btn" />

                    </com.google.android.material.card.MaterialCardView>


                </RelativeLayout>


                <RelativeLayout
                    android:layout_width="36dp"
                    android:layout_height="36dp"
                    android:id="@+id/front_change_btn"
                    android:layout_gravity="center_vertical">

                    <ImageView
                        android:layout_width="18.9dp"
                        android:layout_height="16.2dp"
                        android:background="@drawable/common_2_icon_selfie"
                        android:id="@+id/change_image"
                        android:layout_centerInParent="true"/>

                </RelativeLayout>

            </LinearLayout>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_centerHorizontal="true"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="26.1dp"
                android:id="@+id/guide_btn">

                <ImageView
                    android:layout_width="9.9dp"
                    android:layout_height="10.8dp"
                    android:background="@drawable/common_2_icon_check_guide"
                    android:layout_gravity="center_vertical"
                    android:layout_marginTop="1dp"/>


                <TextView
                    style="@style/Font_Regular_13"
                    android:textSize="12dp"
                    android:textColor="#898989"
                    android:text="@string/eye_check_3"
                    android:layout_marginLeft="4.5dp"
                    android:layout_gravity="center_vertical"/>




            </LinearLayout>


        </RelativeLayout>



    </LinearLayout>

    <View
        android:id="@+id/bg"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#99000000"
        android:visibility="gone" />


    <include layout="@layout/bottom_error_diagpopup_s" />


    <include layout="@layout/loadingparentview_s" />


    <include layout="@layout/pop_skin_selectview_s" />


    <include layout="@layout/activity_guide_skin1_s" />
    <include layout="@layout/activity_guide_skin2_s" />
    <include layout="@layout/activity_guide_skin3_s" />
    <include layout="@layout/activity_guide_skin4_s" />
    <include layout="@layout/activity_guide_skin5_s" />
    <View
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/notclickview"
        android:visibility="gone"/>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/ScreenShotLayout"
        android:background="#dfffffff"
        android:visibility="gone"/>
</androidx.coordinatorlayout.widget.CoordinatorLayout>