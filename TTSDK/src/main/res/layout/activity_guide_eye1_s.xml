<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_guide_eye1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_af000000"
    android:visibility="gone">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/v_515dp"
        android:layout_centerInParent="true"
        android:layout_marginLeft="@dimen/v_30dp"
        android:layout_marginRight="@dimen/v_30dp">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/v_48dp">


        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/v_32dp"
            android:orientation="vertical">

            <TextView
                style="@style/Font_Bold_16"
                android:layout_width="match_parent"
                android:layout_gravity="center_horizontal"
                android:gravity="center_horizontal"
                android:lineHeight="@dimen/v_26dp"
                android:textColor="#191919"
                android:textSize="@dimen/v_19dp" />


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/v_145dp"
                android:layout_marginTop="@dimen/v_30dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="145dp"
                    android:layout_gravity="center_horizontal"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/v_145dp"
                        android:layout_marginLeft="@dimen/v_20dp"
                        android:layout_weight="1"
                        android:background="@drawable/round_radius_17_f2f2f2"
                        android:orientation="vertical">


                        <ImageView
                            android:layout_width="@dimen/v_23dp"
                            android:layout_height="@dimen/v_23dp"
                            android:layout_alignParentRight="true"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginStart="@dimen/v_12dp"
                            android:layout_marginTop="@dimen/v_12dp"
                            android:layout_marginEnd="@dimen/v_12dp"
                            android:layout_marginBottom="@dimen/v_12dp"
                            android:id="@+id/guide1_check_img1"
                            android:background="@drawable/v4_common_check_check_on" />


                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="@dimen/v_23dp"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/guide1_img1"
                                android:layout_width="@dimen/v_50dp"
                                android:layout_height="@dimen/v_50dp"
                                android:layout_gravity="center_horizontal"
                                android:background="@drawable/v4_g_dog_r" />


                            <TextView
                                style="@style/Font_Bold_16"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginTop="@dimen/v_10dp"
                                android:textSize="@dimen/v_16dp"
                                android:text="@string/pop_eye_2"
                                android:textColor="@color/color_191919" />


                            <TextView
                                android:id="@+id/guide1_img_txt"
                                style="@style/Font_Regular_13"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginTop="@dimen/v_2dp"
                                android:text="@string/v4_pop_eye_5"
                                android:textColor="@color/color_898989"
                                android:textSize="@dimen/v_13dp" />

                        </LinearLayout>


                        <com.aiforpet.pet.util.LibraryLottieView
                            android:id="@+id/handloading1_eye"
                            android:layout_width="@dimen/v_60dp"
                            android:layout_height="@dimen/v_60dp"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentBottom="true"
                            app:lottie_autoPlay="true" />


                    </RelativeLayout>

                    <View
                        android:layout_width="@dimen/v_15dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0" />


                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/v_145dp"
                        android:layout_marginRight="@dimen/v_20dp"
                        android:layout_weight="1"
                        android:orientation="vertical">


                    </RelativeLayout>


                </LinearLayout>



            </RelativeLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/v_80dp"
                android:layout_marginLeft="@dimen/v_20dp"
                android:layout_marginTop="@dimen/v_15dp"
                android:layout_marginRight="@dimen/v_20dp"
                android:background="@drawable/round_radius_17_e9e9e9"
                android:orientation="horizontal">


                <TextView
                    android:id="@+id/guide_pop_flash_txt"
                    style="@style/Font_Bold_14"
                    android:textSize="@dimen/v_14dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/v_20dp"
                    android:layout_weight="1"
                    android:gravity="center_vertical" />


                <ImageView
                    android:id="@+id/pop_flash_select"
                    android:layout_width="@dimen/v_47dp"
                    android:layout_height="@dimen/v_28dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/v_20dp"
                    android:layout_marginRight="@dimen/v_20dp"
                    android:layout_weight="0"
                    android:background="@drawable/v4_btn_toggle_on" />


            </LinearLayout>


            <TextView
                android:id="@+id/guide_eye1_txt"
                style="@style/Font_Bold_15"
                android:textSize="@dimen/v_15dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/v_17dp"
                android:paddingBottom="@dimen/v_17dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/v_20dp"
                android:background="@drawable/round_radius_15_ffffff"
                android:gravity="center"
                android:textColor="@color/color_191919" />


        </LinearLayout>









    </RelativeLayout>


</RelativeLayout>