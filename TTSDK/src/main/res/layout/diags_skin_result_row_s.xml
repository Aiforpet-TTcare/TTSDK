<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/v_25dp" />

    <com.google.android.material.card.MaterialCardView

        android:id="@+id/card_row1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/v_20dp"
        android:layout_marginRight="@dimen/v_20dp"
        android:shape="ring"
        app:cardCornerRadius="16dp"
        app:cardElevation="0dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <RelativeLayout
                android:id="@+id/row_1_question"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/v_24dp">


                <TextView
                    android:id="@+id/row_title"
                    style="@style/Font_Bold_17"
                    android:layout_marginLeft="@dimen/v_20dp"
                    android:textColor="#191919" />



                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:background="@drawable/round_30_d4d4d4"
                    android:layout_alignParentRight="true"
                    android:id="@+id/row_question"
                    android:layout_marginRight="@dimen/v_12dp">

                    <TextView
                        style="@style/Font_Regular_13"
                        android:textSize="@dimen/v_12dp"
                        android:text="@string/diags_result_row_2"
                        android:textColor="#717171"
                        android:layout_marginLeft="@dimen/v_18dp"
                        android:layout_marginTop="@dimen/v_5dp"
                        android:layout_marginBottom="@dimen/v_6dp"/>


                    <ImageView
                        android:layout_width="@dimen/v_12dp"
                        android:layout_height="@dimen/v_12dp"
                        android:background="@drawable/ico_tooltip_mini"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/v_6dp"
                        android:layout_marginRight="@dimen/v_12dp"/>



                </LinearLayout>


            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/v_20dp"
                android:layout_marginTop="@dimen/v_16dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal">

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/row1_cardview"
                        android:layout_width="@dimen/v_60dp"
                        android:layout_height="@dimen/v_60dp"
                        android:shape="ring"
                        app:cardBackgroundColor="#ffffff"
                        android:background="#ffffff"
                        app:strokeWidth="2dp"
                        app:cardCornerRadius="17.14dp"
                        app:cardPreventCornerOverlap="false"
                        app:cardElevation="0dp">

                        <ImageView
                            android:id="@+id/row1_image"
                            android:layout_width="@dimen/v_55dp"
                            android:layout_height="@dimen/v_55dp"
                            android:layout_gravity="center" />

                        <ImageView
                            android:id="@+id/row1_image_loading"
                            android:layout_width="@dimen/v_60dp"
                            android:layout_height="@dimen/v_60dp"
                            android:layout_gravity="center" />

                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/v_8dp"
                        android:shape="ring"
                        app:cardCornerRadius="2dp"
                        app:cardElevation="0dp"
                        android:id="@+id/row1_graph">

                        <LinearLayout
                            android:layout_width="@dimen/v_4dp"
                            android:layout_height="@dimen/v_42dp"
                            android:orientation="vertical">

                            <View
                                android:id="@+id/row1_graph_1"
                                android:layout_width="@dimen/v_4dp"
                                android:layout_height="match_parent"
                                android:background="#dcdcdc"
                                android:layout_weight="1"/>

                            <View
                                android:id="@+id/row1_graph_2"
                                android:layout_width="@dimen/v_4dp"
                                android:layout_height="@dimen/v_13dp"
                                android:layout_weight="0"
                                android:background="#dcdcdc" />


                        </LinearLayout>





                    </com.google.android.material.card.MaterialCardView>


                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/v_12dp"
                        android:orientation="vertical">

                        <TextView
                            style="@style/Font_Regular_13"
                            android:textSize="@dimen/v_13dp"
                            android:id="@+id/row1_t1"
                            android:textColor="#717171"
                            android:text="@string/disags_skin_result_1"/>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/row_1_level"
                                style="@style/Font_SF_18"
                                android:text="@string/diags_result_row_3"
                                android:textColor="#ff452b"
                                android:textSize="@dimen/v_20dp" />

                            <TextView
                                android:id="@+id/row_1_level_unit"
                                style="@style/Font_SF_18"
                                android:layout_marginLeft="1dp"
                                android:text="%"
                                android:textColor="#ff452b"
                                android:layout_gravity="bottom"
                                android:textSize="@dimen/v_12dp"
                                android:layout_marginBottom="1dp"/>

                        </LinearLayout>


                    </LinearLayout>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal">

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/row2_cardview"
                        android:layout_width="@dimen/v_60dp"
                        android:layout_height="@dimen/v_60dp"
                        android:shape="ring"
                        app:cardBackgroundColor="#ffffff"
                        android:background="#ffffff"
                        app:strokeWidth="2dp"
                        app:cardCornerRadius="17.14dp"
                        app:cardPreventCornerOverlap="false"
                        app:cardElevation="0dp">

                        <ImageView
                            android:id="@+id/row2_image"
                            android:layout_width="@dimen/v_55dp"
                            android:layout_height="@dimen/v_55dp"
                            android:layout_gravity="center" />

                        <ImageView
                            android:id="@+id/row2_image_loading"
                            android:layout_width="@dimen/v_60dp"
                            android:layout_height="@dimen/v_60dp"
                            android:layout_gravity="center" />

                    </com.google.android.material.card.MaterialCardView>


                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/v_8dp"
                        android:shape="ring"
                        app:cardCornerRadius="2dp"
                        app:cardElevation="0dp"
                        android:id="@+id/row2_graph">

                        <LinearLayout
                            android:layout_width="@dimen/v_4dp"
                            android:layout_height="@dimen/v_42dp"
                            android:orientation="vertical">

                            <View
                                android:id="@+id/row2_graph_1"
                                android:layout_width="@dimen/v_4dp"
                                android:layout_height="match_parent"
                                android:background="#dcdcdc"
                                android:layout_weight="1"/>

                            <View
                                android:id="@+id/row2_graph_2"
                                android:layout_width="@dimen/v_4dp"
                                android:layout_height="@dimen/v_13dp"
                                android:layout_weight="0"
                                android:background="#dcdcdc" />


                        </LinearLayout>





                    </com.google.android.material.card.MaterialCardView>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/v_12dp"
                        android:orientation="vertical">

                        <TextView
                            style="@style/Font_Regular_13"
                            android:textSize="@dimen/v_13dp"
                            android:id="@+id/row2_t1"
                            android:textColor="#717171"
                            android:text="@string/disags_skin_result_2"/>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/row_2_level"
                                style="@style/Font_SF_18"
                                android:text="@string/diags_result_row_3"
                                android:textColor="#ff452b"
                                android:textSize="@dimen/v_20dp" />

                            <TextView
                                android:id="@+id/row_2_level_unit"
                                style="@style/Font_SF_18"
                                android:layout_marginLeft="1dp"
                                android:text="%"
                                android:textColor="#ff452b"
                                android:layout_gravity="bottom"
                                android:textSize="@dimen/v_12dp"
                                android:layout_marginBottom="1dp"/>

                        </LinearLayout>


                    </LinearLayout>


                </LinearLayout>





            </LinearLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/v_20dp"
                android:layout_marginTop="@dimen/v_16dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal">

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/row3_cardview"
                        android:layout_width="@dimen/v_60dp"
                        android:layout_height="@dimen/v_60dp"
                        android:shape="ring"
                        app:cardBackgroundColor="#ffffff"
                        android:background="#ffffff"
                        app:strokeWidth="2dp"
                        app:cardCornerRadius="17.14dp"
                        app:cardPreventCornerOverlap="false"
                        app:cardElevation="0dp">

                        <ImageView
                            android:id="@+id/row3_image"
                            android:layout_width="@dimen/v_55dp"
                            android:layout_height="@dimen/v_55dp"
                            android:layout_gravity="center" />

                        <ImageView
                            android:id="@+id/row3_image_loading"
                            android:layout_width="@dimen/v_60dp"
                            android:layout_height="@dimen/v_60dp"
                            android:layout_gravity="center" />

                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="8.2dp"
                        android:shape="ring"
                        app:cardCornerRadius="2dp"
                        app:cardElevation="0dp"
                        android:id="@+id/row3_graph">

                        <LinearLayout
                            android:layout_width="@dimen/v_4dp"
                            android:layout_height="@dimen/v_42dp"
                            android:orientation="vertical">

                            <View
                                android:id="@+id/row3_graph_1"
                                android:layout_width="@dimen/v_4dp"
                                android:layout_height="match_parent"
                                android:background="#dcdcdc"
                                android:layout_weight="1"/>

                            <View
                                android:id="@+id/row3_graph_2"
                                android:layout_width="@dimen/v_4dp"
                                android:layout_height="@dimen/v_13dp"
                                android:layout_weight="0"
                                android:background="#dcdcdc" />


                        </LinearLayout>




                    </com.google.android.material.card.MaterialCardView>


                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/v_12dp"
                        android:orientation="vertical">

                        <TextView
                            style="@style/Font_Regular_13"
                            android:textSize="@dimen/v_13dp"
                            android:id="@+id/row3_t1"
                            android:textColor="#717171"
                            android:text="@string/disags_skin_result_3"/>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/row_3_level"
                                style="@style/Font_SF_18"
                                android:text="@string/diags_result_row_3"
                                android:textColor="#ff452b"
                                android:textSize="@dimen/v_20dp" />

                            <TextView
                                android:id="@+id/row_3_level_unit"
                                style="@style/Font_SF_18"
                                android:layout_marginLeft="1dp"
                                android:text="%"
                                android:textColor="#ff452b"
                                android:layout_gravity="bottom"
                                android:textSize="@dimen/v_12dp"
                                android:layout_marginBottom="1dp"/>

                        </LinearLayout>


                    </LinearLayout>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal">

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/row4_cardview"
                        android:layout_width="@dimen/v_60dp"
                        android:layout_height="@dimen/v_60dp"
                        app:cardBackgroundColor="#ffffff"
                        android:background="#ffffff"
                        app:strokeWidth="2dp"
                        app:cardCornerRadius="17.14dp"
                        app:cardPreventCornerOverlap="false"
                        app:cardElevation="0dp">

                        <ImageView
                            android:id="@+id/row4_image"
                            android:layout_width="@dimen/v_55dp"
                            android:layout_height="@dimen/v_55dp"
                            android:layout_gravity="center" />
                        <ImageView
                            android:id="@+id/row4_image_loading"
                            android:layout_width="@dimen/v_60dp"
                            android:layout_height="@dimen/v_60dp"
                            android:layout_gravity="center" />

                    </com.google.android.material.card.MaterialCardView>


                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/v_8dp"
                        android:shape="ring"
                        app:cardCornerRadius="2dp"
                        app:cardElevation="0dp"
                        android:id="@+id/row4_graph">

                        <LinearLayout
                            android:layout_width="@dimen/v_4dp"
                            android:layout_height="@dimen/v_42dp"
                            android:orientation="vertical">

                            <View
                                android:id="@+id/row4_graph_1"
                                android:layout_width="@dimen/v_4dp"
                                android:layout_height="match_parent"
                                android:background="#dcdcdc"
                                android:layout_weight="1"/>

                            <View
                                android:id="@+id/row4_graph_2"
                                android:layout_width="@dimen/v_4dp"
                                android:layout_height="@dimen/v_13dp"
                                android:layout_weight="0"
                                android:background="#dcdcdc" />


                        </LinearLayout>




                    </com.google.android.material.card.MaterialCardView>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="12dp"
                        android:orientation="vertical">

                        <TextView
                            style="@style/Font_Regular_13"
                            android:textSize="@dimen/v_13dp"
                            android:id="@+id/row4_t1"
                            android:textColor="#717171"
                            android:text="@string/disags_skin_result_4"/>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/row_4_level"
                                style="@style/Font_SF_18"
                                android:text="@string/diags_result_row_3"
                                android:textColor="#ff452b"
                                android:textSize="@dimen/v_20dp" />

                            <TextView
                                android:id="@+id/row_4_level_unit"
                                style="@style/Font_SF_18"
                                android:layout_marginLeft="1dp"
                                android:text="%"
                                android:textColor="#ff452b"
                                android:layout_gravity="bottom"
                                android:textSize="@dimen/v_12dp"
                                android:layout_marginBottom="1dp"/>

                        </LinearLayout>


                    </LinearLayout>


                </LinearLayout>





            </LinearLayout>




            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/row_doctor_view"
                android:orientation="vertical"
                android:visibility="gone">


                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginRight="@dimen/v_20dp"
                    android:layout_marginLeft="@dimen/v_20dp"
                    android:background="#e6e6e6"
                    android:layout_marginTop="@dimen/v_24dp"/>


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/v_20dp"
                    android:layout_marginTop="@dimen/v_24dp"
                    android:layout_marginLeft="@dimen/v_20dp"
                    android:layout_marginRight="@dimen/v_20dp">



                    <TextView
                        android:id="@+id/t1"
                        style="@style/Font_Bold_13"
                        android:textSize="@dimen/v_13dp"
                        android:layout_centerVertical="true"
                        android:text="@string/diag_doctor_title"
                        android:textColor="#191919" />

                    <ImageView
                        android:layout_width="@dimen/v_16dp"
                        android:layout_height="@dimen/v_18dp"
                        android:layout_marginLeft="2dp"
                        android:layout_centerVertical="true"
                        android:background="@drawable/emoji_bubble_s"
                        android:layout_toRightOf="@+id/t1"/>


                </RelativeLayout>

                <TextView
                    style="@style/Font_Regular_13"
                    android:textSize="@dimen/v_13dp"
                    android:id="@+id/row_doctor_ment"
                    android:textColor="#191919"
                    android:lineHeight="@dimen/v_23dp"
                    android:layout_marginLeft="@dimen/v_20dp"
                    android:layout_marginRight="@dimen/v_20dp"
                    android:layout_marginTop="@dimen/v_5dp"/>


            </LinearLayout>






            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/v_24dp" />


        </LinearLayout>


    </com.google.android.material.card.MaterialCardView>
</LinearLayout>

