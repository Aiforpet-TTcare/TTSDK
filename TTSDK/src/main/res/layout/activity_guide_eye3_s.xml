<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#af000000"
    android:id="@+id/activity_guide_eye3"
    android:visibility="gone">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="59.4dp">

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:visibility="visible"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="45dp"
                android:id="@+id/a3">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="9dp"
                        android:layout_height="8.1dp"
                        android:layout_marginTop="9.9dp"
                        android:layout_gravity="center_horizontal" />

                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="41.4dp"
                        android:layout_height="41.4dp"
                        android:layout_marginTop="5.4dp"
                        android:shape="ring"
                        app:cardBackgroundColor="@android:color/transparent"
                        app:cardCornerRadius="14dp"
                        app:cardElevation="0dp"
                        app:cardPreventCornerOverlap="true">

                    </com.google.android.material.card.MaterialCardView>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="7.2dp"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="9dp"
                        android:layout_height="8.1dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="9.9dp"/>

                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="41.4dp"
                        android:layout_height="41.4dp"
                        android:layout_marginTop="5.4dp"
                        android:shape="ring"
                        app:cardBackgroundColor="@android:color/transparent"
                        app:cardCornerRadius="14dp"
                        app:cardElevation="0dp"
                        app:cardPreventCornerOverlap="true">


                        <ImageView
                            android:layout_width="41.4dp"
                            android:layout_height="41.4dp"/>

                    </com.google.android.material.card.MaterialCardView>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="55.8dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="55.8dp"
                        android:layout_height="24.3dp"
                        android:layout_gravity="center_horizontal"
                        android:background="@drawable/bubble_edit" />

                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="41.4dp"
                        android:layout_height="41.4dp"
                        android:shape="ring"
                        app:cardBackgroundColor="@android:color/transparent"
                        app:cardCornerRadius="14dp"
                        app:cardElevation="0dp"
                        app:cardPreventCornerOverlap="true"
                        android:layout_gravity="center_horizontal">


                        <ImageView
                            android:layout_width="41.4dp"
                            android:layout_height="41.4dp"
                            android:background="@drawable/common_check_again"/>

                    </com.google.android.material.card.MaterialCardView>


                </LinearLayout>


            </LinearLayout>

            <RelativeLayout
                android:layout_width="171dp"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="18dp"
                android:layout_alignParentBottom="true">

                <com.aiforpet.pet.util.LibraryLottieView
                    android:id="@+id/handloading3_eye"
                    android:layout_width="54dp"
                    android:layout_height="54.63dp"
                    app:lottie_imageAssetsFolder="images"
                    app:lottie_assetFileName="han.json"
                    app:lottie_autoPlay="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="21.6dp"/>

            </RelativeLayout>

            <TextView
                style="@style/Font_Bold_15"
                android:textSize="14dp"
                android:layout_width="match_parent"
                android:layout_height="72dp"
                android:layout_above="@+id/a3"
                android:layout_marginBottom="18dp"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="18dp"
                android:layout_marginRight="18dp"
                android:gravity="center"
                android:textColor="@color/color_191919"
                android:id="@+id/guide_eye3_txt"
                android:background="@drawable/round_radius_15_ffffff"/>

        </RelativeLayout>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="171dp">



        </RelativeLayout>



    </LinearLayout>




</RelativeLayout>
